{% if settings.custom_heading_font_family != blank %}
  <style>
    @font-face {
      font-family: {{ settings.custom_heading_font_family }};
      font-style: normal;
      font-display: swap;
      src: url('{{ settings.custom_heading_font | asset_url }}') format('woff');
    }
  </style>
{% endif %}
{% if settings.custom_heading_font_family_other_headings != blank %}
  <style>
    @font-face {
      font-family: {{ settings.custom_heading_font_family_other_headings }};
      font-style: normal;
      font-display: swap;
      src: url('{{ settings.custom_heading_font_other_headings | asset_url }}') format('woff');
    }
  </style>
{% endif %}
{% if settings.custom_body_font_family != blank %}
  <style>
    @font-face {
    font-family: {{ settings.custom_body_font_family }};
    font-style: normal;
    font-display: swap;
    src: url('{{ settings.custom_body_font | asset_url }}') format('woff');
    }
  </style>
{% endif %}
