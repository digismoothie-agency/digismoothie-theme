        "testimonial_GiTPex": {
          "type": "testimonial",
          "settings": {
            "rating": "5-stars",
            "text": "<p>Super, rychle dodání. Úžasná věc. Krásné vzpomínky na dětství. Prstýnky na každý prst jsou super!<\/p>",
            "product": "",
            "author": "Helena S., Ověřený zákazník",
