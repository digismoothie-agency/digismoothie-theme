        "testimonial_KM89VL": {
          "type": "testimonial",
          "settings": {
            "rating": "5-stars",
            "text": "<p>Naprosto dokonalé, jak krekry, tak komunikace s obchodem, který nikdy nezklame a proto doporučuji všem!!!<\/p>",
